<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/background_ffffffff_rectangle_10">

    <TextView
        android:id="@+id/credential_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_23"
        android:layout_marginEnd="@dimen/dp_23"
        android:textColor="@color/black_ff333333"
        android:textSize="@dimen/dp_15"
        android:textStyle="bold" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginTop="@dimen/dp_9"
        android:background="@color/white_ffebecf0" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_19"
        android:layout_marginStart="@dimen/dp_23"
        android:layout_marginEnd="@dimen/dp_23"
        android:text="@string/public_key_hash"
        android:textColor="@color/gray_ff85899c"
        android:textSize="@dimen/dp_13"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/public_key_hash"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_23"
        android:layout_marginEnd="@dimen/dp_23"
        android:textColor="@color/black_ff333333"
        android:textSize="@dimen/dp_15"
        android:textStyle="normal" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_19"
        android:layout_marginStart="@dimen/dp_23"
        android:layout_marginEnd="@dimen/dp_23"
        android:text="@string/storage_location"
        android:textColor="@color/gray_ff85899c"
        android:textSize="@dimen/dp_13"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/storage_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_23"
        android:layout_marginEnd="@dimen/dp_23"
        android:textColor="@color/black_ff333333"
        android:textSize="@dimen/dp_15"
        android:textStyle="normal" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_19"
        android:layout_marginStart="@dimen/dp_23"
        android:layout_marginEnd="@dimen/dp_23"
        android:text="@string/last_update_time"
        android:textColor="@color/gray_ff85899c"
        android:textSize="@dimen/dp_13"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/last_update_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginBottom="@dimen/dp_19"
        android:layout_marginStart="@dimen/dp_23"
        android:layout_marginEnd="@dimen/dp_23"
        android:textColor="@color/black_ff333333"
        android:textSize="@dimen/dp_15"
        android:textStyle="normal" />
</LinearLayout>