<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 第一层 -->
    <item>
        <shape>
            <solid android:color="@color/white_ffffffff" />
            <!-- 圆角 -->
            <corners android:radius="@dimen/dp_10" />
        </shape>
    </item>
    <!-- 第二层 -->
    <!-- 距离第一层上边缘 -->
    <item android:top="@dimen/dp_10">
        <shape>
            <!-- 实心填充颜色 -->
            <solid android:color="@color/white_ffffffff"/>
        </shape>
    </item>

</layer-list>